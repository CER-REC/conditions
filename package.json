{
  "name": "neb-conditions-visualization",
  "version": "0.0.1",
  "description": "A visualization of data collected since 2010 about conditions on energy projects that the NEB creates as part of their regulatory process. ",
  "main": "app/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "npm run build:data && npm run build:webpack",
    "build:data": "echo \"Error: not defined\" && exit 1",
    "build:webpack": "echo \"Error: not defined\" && exit 1",
    "lint": "eslint ./app/**/*.js{,x} ./server/**/*.js",
    "start": "babel-node ./server/index.js",
    "test": "cross-env NODE_ENV=test nyc npm run test:raw",
    "test:raw": "cross-env NODE_ENV=test mocha --require babel-register --require test/test_helper.js --recursive app/**/*.spec.jsx",
    "test:single": "cross-env NODE_ENV=test mocha --require babel-register --require test/test_helper.js --recursive",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "Spenser Jones <hello@spenserj.com>",
  "license": "Open Government Liscense - Canada",
  "dependencies": {
    "apollo-boost": "^0.1.20",
    "dialog-polyfill": "^0.4.9",
    "domready": "^1.0.8",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.8.0",
    "immutable": "^3.8.1",
    "lodash.memoize": "^4.1.2",
    "prop-types": "^15.6.0",
    "re-reselect": "^2.0.0",
    "react": "^16.6.0",
    "react-apollo": "^2.2.4",
    "react-dom": "^16.6.0",
    "react-hot-loader": "^4.2.0",
    "react-redux": "^5.0.5",
    "redux": "^3.7.2",
    "reselect": "^3.0.1",
    "reselect-tools": "0.0.6"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^4.0.2",
    "@storybook/addon-actions": "^4.0.2",
    "@storybook/addon-info": "^4.0.4",
    "@storybook/addon-knobs": "^4.0.2",
    "@storybook/addon-links": "^4.0.2",
    "@storybook/addon-notes": "^4.0.2",
    "@storybook/addon-options": "^4.0.2",
    "@storybook/addon-viewport": "^4.0.2",
    "@storybook/addons": "^4.0.2",
    "@storybook/react": "^4.0.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "express": "^4.15.3",
    "file-loader": "^2.0.0",
    "jsdom": "^13.0.0",
    "mocha": "^5.1.1",
    "mocha-webpack": "^1.1.0",
    "mustache": "^2.3.0",
    "mustache-express": "^1.2.5",
    "node-sass": "^4.5.3",
    "nsp": "^2.7.0",
    "nyc": "^11.7.3",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.6",
    "sinon": "^5.0.7",
    "storybook-addon-specifications": "^2.1.2",
    "storybook-readme": "^4.0.2",
    "style-loader": "^0.18.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.25.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.6.1",
    "webpack-hot-middleware": "^2.18.2"
  },
  "nyc": {
    "require": [
      "babel-register",
      "./test/test_helper.js"
    ],
    "exclude": [
      "**/*.stories.js",
      "**/*.stories.jsx",
      "**/*.spec.js",
      "**/*.spec.jsx",
      "coverage/**",
      "**/node_modules/**",
      "devPublic",
      ".cache"
    ],
    "include": [
      "app/**/*"
    ],
    "extension": [
      ".jsx"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "sourceMap": false,
    "instrument": false,
    "cache": true,
    "all": true
  }
}
