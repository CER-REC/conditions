{
  "name": "neb-conditions-visualization",
  "version": "0.0.1",
  "description": "A visualization of data collected since 2010 about conditions on energy projects that the NEB creates as part of their regulatory process. ",
  "main": "app/index.js",
  "scripts": {
    "ci:test": "npm run test -- --reporter mocha-junit-reporter --reporter-options mochaFile=./gl-junit.xml && npm run ci:testcoverage",
    "ci:testcoverage": "nyc report --reporter=text-lcov > coverage.lcov && npx codecov",
    "cz": "git-cz",
    "lint": "eslint -f gitlab \"./app/**/*.js\" \"./app/**/*.jsx\" \"./server/**/*.js\"",
    "test": "cross-env NODE_ENV=test jest",
    "test:single": "cross-env NODE_ENV=test mocha --opts app/tests/mocha.opts",
    "test:showcoverage": "opn coverage/lcov-report/index.html",
    "start": "babel-node ./server/index.js",
    "storybook": "start-storybook -s ./app/images -p 6006",
    "build-storybook": "build-storybook -s ./app/images",
    "postshrinkwrap": "replace --silent 'http://' 'https://' ./package-lock.json"
  },
  "author": "Spenser Jones <hello@spenserj.com>",
  "license": "Open Government License - Canada",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-brands-svg-icons": "^5.6.3",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "classnames": "^2.2.6",
    "d3": "^5.7.0",
    "dialog-polyfill": "^0.4.10",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "immutable": "^4.0.0-rc.12",
    "lodash.memoize": "^4.1.2",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-intl": "^2.7.2",
    "react-redux": "^6.0.0",
    "react-spring": "^7.2.8",
    "redux": "^4.0.1",
    "victory": "^31.0.2"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/node": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "7.2.0",
    "@babel/polyfill": "7.2.5",
    "@babel/preset-env": "7.2.3",
    "@babel/preset-react": "7.0.0",
    "@storybook/addon-a11y": "4.1.7",
    "@storybook/addon-info": "4.1.7",
    "@storybook/addon-knobs": "4.1.7",
    "@storybook/addon-options": "4.1.7",
    "@storybook/addon-storyshots": "4.1.7",
    "@storybook/addon-viewport": "4.1.7",
    "@storybook/addons": "4.1.7",
    "@storybook/react": "4.1.7",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.5",
    "babel-plugin-add-react-displayname": "0.0.5",
    "babel-plugin-inline-import": "3.0.0",
    "babel-plugin-istanbul": "5.1.0",
    "babel-plugin-macros": "2.4.5",
    "babel-plugin-react-docgen": "2.0.0",
    "babel-plugin-require-context-hook": "1.0.0",
    "chai": "4.2.0",
    "codecov": "3.1.0",
    "commitizen": "3.0.5",
    "cross-env": "5.2.0",
    "css-loader": "2.1.0",
    "cz-customizable": "5.3.0",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "eslint": "5.12.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-formatter-gitlab": "1.0.2",
    "eslint-junit": "1.0.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.12.4",
    "express": "4.16.4",
    "file-loader": "3.0.1",
    "gitlab-npm-audit-parser": "1.0.2",
    "jest": "23.6.0",
    "jsdom": "13.1.0",
    "jsdom-global": "3.0.2",
    "mocha": "5.2.0",
    "mocha-junit-reporter": "1.18.0",
    "mustache": "3.0.1",
    "mustache-express": "1.2.8",
    "node-sass": "4.11.0",
    "nyc": "13.1.0",
    "opn-cli": "4.0.0",
    "raw-loader": "1.0.0",
    "react-test-renderer": "16.7.0",
    "replace": "1.0.1",
    "require-context.macro": "1.0.4",
    "sass-loader": "7.1.0",
    "sinon": "7.2.2",
    "storybook-addon-interaction": "0.0.1",
    "storybook-addon-intl": "2.3.2",
    "storybook-readme": "4.0.5",
    "style-loader": "0.23.1",
    "url-loader": "1.1.2",
    "webpack": "4.28.4",
    "webpack-dev-middleware": "3.5.1",
    "webpack-dev-server": "3.1.14",
    "webpack-hot-middleware": "2.24.3"
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "exclude": [
      "**/node_modules/**",
      ".cache/",
      ".nyc_output",
      "app/tests/",
      "coverage",
      "devPublic/",
      "documentation",
      "storybook-static",
      "test-reports",
      "webpack.config.js",
      "**/spec.js",
      "**/spec.jsx",
      "**/*.spec.js",
      "**/*.spec.jsx",
      "**/stories.jsx",
      "app/components/Modal/index.jsx"
    ],
    "extension": [
      ".js",
      ".jsx"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "sourceMap": false,
    "instrument": true,
    "cache": true,
    "all": true
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "eslint-junit": {
    "suiteName": "ESLint",
    "output": "./gl-junit.xml"
  }
}
