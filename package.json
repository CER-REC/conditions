{
  "name": "neb-conditions-visualization",
  "version": "0.3.0",
  "description": "A visualization of data collected since 2010 about conditions on energy projects that the NEB creates as part of their regulatory process. ",
  "main": "src/index.jsx",
  "scripts": {
    "ci:test": "npm run test -- --ci --reporters=default --reporters=jest-junit && codecov",
    "cz": "git-cz",
    "lint": "eslint -f gitlab --ext .js,.jsx .",
    "test": "cross-env NODE_ENV=test jest",
    "test:showcoverage": "opn coverage/lcov-report/index.html",
    "start": "npm run storybook",
    "storybook": "start-storybook -s ./src/images -p 6006",
    "build-storybook": "build-storybook -s ./src/images",
    "build:modified": "git show -s --format=%cd HEAD --date=short > LAST_MODIFIED",
    "build:update-version": "node -p -e \"require('./package.json').version\" > VERSION",
    "build:changelog": "conventional-changelog -n .release/index.js -i CHANGELOG.md -s",
    "build": "webpack && npm run build:modified && npm run build:update-version",
    "version": "npm run build:changelog && git add CHANGELOG.md"
  },
  "contributors": [
    "Brandon Devaleriola",
    "Charlie Cheung",
